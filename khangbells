local runScript = true
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
local frame = Instance.new("Frame")
local textLabel = Instance.new("TextLabel")
local okButton = Instance.new("TextButton")
local textBox = Instance.new("TextBox")
local imageLabel = Instance.new("ImageLabel")

screenGui.Parent = player.PlayerGui
screenGui.Name = "ScriptNotification"

imageLabel.Parent = screenGui
imageLabel.Size = UDim2.new(1, 0, 1, 0)
imageLabel.Position = UDim2.new(0, 0, 0, 0)
imageLabel.Image = "rbxassetid://YOUR_IMAGE_ID"
imageLabel.ImageTransparency = 0

frame.Parent = screenGui
frame.Size = UDim2.new(0, 400, 0, 200)
frame.Position = UDim2.new(0.5, -200, 0.5, -100)
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)

textLabel.Parent = frame
textLabel.Size = UDim2.new(1, 0, 0.5, 0)
textLabel.Position = UDim2.new(0, 0, 0, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "khangbells run"
textLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
textLabel.TextScaled = true
textLabel.TextSize = 36
textLabel.TextFont = Enum.Font.SourceSansBold

textBox.Parent = frame
textBox.Size = UDim2.new(0, 300, 0, 30)
textBox.Position = UDim2.new(0.5, -150, 0.6, 0)
textBox.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
textBox.Text = ""
textBox.PlaceholderText = "Nhập gì đó nếu muốn..."
textBox.TextSize = 18
textBox.ClearTextOnFocus = false

okButton.Parent = frame
okButton.Size = UDim2.new(0, 80, 0, 40)
okButton.Position = UDim2.new(0.5, -40, 0.8, 0)
okButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
okButton.Text = "X"
okButton.TextColor3 = Color3.fromRGB(255, 255, 255)
okButton.TextScaled = true

okButton.MouseButton1Click:Connect(function()
    if textBox.Text ~= "" then
        print("User input: " .. textBox.Text)
    end
    screenGui:Destroy()
    runScript = true
end)

function toggleScript()
    if runScript then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/thaibao/main/TbaoHubRivals"))()
        print("khangbells run")
    else
        player:Kick("khangbells script by sonbui offline")
    end
end

toggleScript()
